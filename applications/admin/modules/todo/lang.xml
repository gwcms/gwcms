<![CDATA[						
[
	{
		"name":"Eilė",
		"conditions":"state<100 AND type IN (1,0)",
		"calculate":1,
		"default":1
	},
	{
		"name":"Atlikti",
		"conditions":"state>=100 AND type IN (1,0)",
		"calculate":1
	}
]
]]>						
